<%@ page language="java" contentType="text/plain; charset=UTF-8" pageEncoding="UTF-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><%@ taglib uri="http://java.sun.com/jsp/jstl/fmt" prefix="fmt"%><c:set var="campText" value="${camp.dominiCampText}"/><c:if test="${not empty camp.enumeracio}"><c:set var="campText" value="valor"/></c:if><c:set var="campValor" value="${camp.dominiCampValor}"/><c:if test="${not empty camp.enumeracio}"><c:set var="campValor" value="codi"/></c:if>[<c:forEach var="fila" items="${resultat}" varStatus="status">{<c:forEach var="columna" items="${fila.columnes}"><c:if test="${columna.codi == campText}"><fmt:message key='domini.select.text' />:"${columna.valor}"</c:if></c:forEach><c:forEach var="columna" items="${fila.columnes}"><c:if test="${columna.codi == campValor}">,<fmt:message key='domini.select.valor' />:"${columna.valor}"</c:if></c:forEach>}<c:if test="${not status.last}">,</c:if></c:forEach>]